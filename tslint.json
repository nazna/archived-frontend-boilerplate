{
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "member-access": false,
    "no-console": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "prettier": true
  }
}
